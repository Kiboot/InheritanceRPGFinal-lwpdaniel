<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lolwallpaper"
    tools:context=".XMonone">

    <TextView
        android:id="@+id/daPhysDMG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="84dp"
        android:text="outputphysdmg"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daMP" />

    <TextView
        android:id="@+id/teMgcDMG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Mgc DMG"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim4"
        app:layout_constraintTop_toBottomOf="@+id/tePhysDMG" />

    <TextView
        android:id="@+id/tePhysDMG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Phys DMG"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim3"
        app:layout_constraintTop_toBottomOf="@+id/teMP" />

    <TextView
        android:id="@+id/daDes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="outputdescription"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daMgcDEF" />

    <TextView
        android:id="@+id/teHP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="HP"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim1"
        app:layout_constraintTop_toBottomOf="@+id/daLevel" />

    <TextView
        android:id="@+id/daPhysDEF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="84dp"
        android:text="outputphysdef"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daMgcDMG" />

    <TextView
        android:id="@+id/daMgcDMG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="84dp"
        android:text="outputmgcdmg"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daPhysDMG" />

    <TextView
        android:id="@+id/teMP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="MP"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim2"
        app:layout_constraintTop_toBottomOf="@+id/teHP" />

    <TextView
        android:id="@+id/teMgcDEF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Magic DEF"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim6"
        app:layout_constraintTop_toBottomOf="@+id/tePhysDEF" />

    <TextView
        android:id="@+id/tePhysDEF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Physical DEF"
        android:textColor="#e9dda2"
        app:layout_constraintStart_toEndOf="@+id/monim5"
        app:layout_constraintTop_toBottomOf="@+id/teMgcDMG" />

    <TextView
        android:id="@+id/daLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="outputlevel"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daName" />

    <TextView
        android:id="@+id/daHP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="84dp"
        android:text="outputhp"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daLevel" />

    <TextView
        android:id="@+id/daMP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="84dp"
        android:text="outputmp"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daHP" />

    <TextView
        android:id="@+id/daMgcDEF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="84dp"
        android:text="outputmgcdef"
        android:textColor="#e9dda2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daPhysDEF" />

    <TextView
        android:id="@+id/daName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Name"
        android:textColor="#e9dda2"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Mononeim" />

    <ImageView
        android:id="@+id/Mononeim"
        android:layout_width="230dp"
        android:layout_height="206dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/monone" />

    <ImageView
        android:id="@+id/monim1"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daLevel"
        app:srcCompat="@drawable/hp" />

    <ImageView
        android:id="@+id/monim2"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monim1"
        app:srcCompat="@drawable/mp" />

    <ImageView
        android:id="@+id/monim3"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monim2"
        app:srcCompat="@drawable/atk" />

    <ImageView
        android:id="@+id/monim4"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monim3"
        app:srcCompat="@drawable/mgcatk" />

    <ImageView
        android:id="@+id/monim5"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monim4"
        app:srcCompat="@drawable/def" />

    <ImageView
        android:id="@+id/monim6"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monim5"
        app:srcCompat="@drawable/mgcdef" />

</androidx.constraintlayout.widget.ConstraintLayout>