<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lolwallpaper"
    tools:context=".Secondload">

    <ImageView
        android:id="@+id/theLogo2"
        android:layout_width="171dp"
        android:layout_height="167dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/exploye" />

    <TextView
        android:id="@+id/theTitle2"
        android:layout_width="261dp"
        android:layout_height="37dp"
        android:fontFamily="@font/frizquadrata"
        android:text="Exploy Developer"
        android:textAlignment="center"
        android:textColor="#e9dda2"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/theLogo2"
        app:layout_constraintStart_toStartOf="@+id/theLogo2"
        app:layout_constraintTop_toBottomOf="@+id/theLogo2" />

    <ImageView
        android:id="@+id/theLogo3"
        android:layout_width="171dp"
        android:layout_height="167dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/theTitle2"
        app:srcCompat="@drawable/kblogo" />

    <TextView
        android:id="@+id/theTitle3"
        android:layout_width="261dp"
        android:layout_height="37dp"
        android:fontFamily="@font/frizquadrata"
        android:text="Kogiebags Production"
        android:textAlignment="center"
        android:textColor="#e9dda2"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/theLogo3"
        app:layout_constraintStart_toStartOf="@+id/theLogo3"
        app:layout_constraintTop_toBottomOf="@+id/theLogo3" />

</androidx.constraintlayout.widget.ConstraintLayout>